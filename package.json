{
  "name": "KarmaNative",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "npm run test:lint; npm run test:unit",
    "dev": "npm run dev:ios",
    "dev:ios": "react-native run-ios",
    "dev:android": "react-native run-android",
    "test:lint": "./node_modules/.bin/eslint . --color",
    "test:unit": "./node_modules/.bin/jest  --verbose",
    "test:watch": "./node_modules/.bin/jest --watchAll --verbose",
    "sos": "node sos.js",
    "purge": "rm -rf node_modules; npm cache clean; npm i"
  },
  "dependencies": {
    "prop-types": "15.5.9",
    "react": "16.0.0-alpha.12",
    "react-native": "0.47.0",
    "react-navigation": "1.0.0-beta.12",
    "react-redux": "5.0.4",
    "redux": "3.6.0",
    "redux-logger": "3.0.1",
    "redux-multi": "0.1.12",
    "redux-thunk": "2.2.0",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-react-native": "2.1.0",
    "eslint-config-standard": "10.2.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-node": "4.2.2",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-react": "7.0.0",
    "eslint-plugin-standard": "3.0.1",
    "eslint": "3.19.0",
    "jest": "20.0.4",
    "nock": "9.0.13",
    "react-test-renderer": "16.0.0-alpha.12",
    "shelljs": "^0.7.7"
  },
  "jest": {
    "preset": "react-native"
  }
}
